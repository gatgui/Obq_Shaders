## LOGIC

Language = "Python";
Logic
{
#Coordinates System
def	coordinateSystem_OnChanged():
	if PPG.coordinateSystem.Value == "Custom":
		PPG.customCoordinates.Enable(True)
	else:
		PPG.customCoordinates.Enable(False)


#Tweaks
def tweakEnable0_OnChanged():
	if PPG.tweakEnable0.Value == True:
		PPG.tweakName0.Show(True)
		if PPG.tweakColor0.ReadOnly == False:
			PPG.tweakColor0.Show(True)
		if PPG.tweakScalar0.ReadOnly == False:
			PPG.tweakScalar0.Show(True)
	else:
		PPG.tweakName0.Show(False)
		PPG.tweakColor0.Show(False)
		PPG.tweakScalar0.Show(False)
		
def tweakEnable1_OnChanged():
	if PPG.tweakEnable1.Value == True:
		PPG.tweakName1.Show(True)
