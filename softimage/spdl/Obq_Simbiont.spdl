def btn_replaceLibsPath_OnClicked():
	path = XSIUIToolkit.PickFolder("C:\\", "Pick DTE Component Libs Folder")
	if len(path) > 0:
		oLibs = Application.ActiveProject.ActiveScene.MaterialLibraries
		for lib in oLibs:
			for mat in lib.Items:
				Application.LogMessage(mat)
				allShaders = mat.GetAllShaders()
				for n in allShaders:
					if n.Parameters.GetAsText().find("componentLibsPath")>-1:
						Application.SetValue(n.FullName + ".componentLibsPath", path, "")

